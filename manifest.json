{
  "manifest_version": 3,
  "name": "Source Toolkit",
  "description": "获取网站信息",
  "version": "1.0",
  "action": {
    "default_popup": "popup.html",
    "default_icon": "popup.png"
  },
  "permissions": [
    "activeTab",
    "tabs",
    "webNavigation"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "chrome_url_overrides": {
    "newtab": "new-tab.html"
  },
  "host_permissions": [
    "http://*/*",
    "https://*/*"
  ]
}